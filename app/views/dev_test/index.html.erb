<h1>DevTest#index</h1>

<% if crew_admin_signed_in? %>
<ul>
  <li><h3><%= current_crew_admin.email %></h3></li>

  <li><%= link_to "Logout", destroy_crew_admin_session_path, method: :delete%></li>

</ul>

  <% else %>

<ul>
  <li><%= link_to "Login", new_admin_session_path %></li>
</ul>


<% end %>


<% if user_signed_in? %>
<ul>
  <li><h3><%= current_user.name %></h3></li>
  <li><%= link_to "Logout", destroy_user_session_path, method: :delete%></li>


</ul>

  <% else %>

<ul>
  <li><%= link_to "Login", new_user_session_path %></li>
</ul>


<% end %>


<p>Find me in app/views/dev_test/index.html.erb</p>
