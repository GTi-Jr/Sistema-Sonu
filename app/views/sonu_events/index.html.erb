<p id="notice"><%= notice %></p>

<h1>Sonu Events</h1>
<% if admin_signed_in? %>

<p><%= current_admin.email? %></p>

<%end%>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sonu_events.each do |sonu_event| %>
      <tr>
        <td><%= sonu_event.name %></td>
        <td><%= sonu_event.info %></td>
        <td><%= link_to 'Show', sonu_event %></td>
        <td><%= link_to 'Edit', edit_sonu_event_path(sonu_event) %></td>
        <td><%= link_to 'Destroy', sonu_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sonu Event', new_sonu_event_path %>
